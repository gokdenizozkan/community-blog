<h1>Welcome</h1>

<%= link_to "Write a new article!", new_article_path if user_signed_in? %>
<ul>
  <% @published_articles.each do |article| %>
    <li>
      <%= link_to article.title, article %>
      <%= time_tag article.created_at, "data-local": "time-ago" %>
    </li>
  <% end %>
</ul>